package ej1;

public class InvalidLineFormatException extends Exception {
	
	public void checkLine(String _line, int num) {
		if (_line.matches("^[a-zA-Z0-9\\s]+$")) {
			System.out.println("Línea válida: " + _line);
		} else {
			System.out.println("Error: Formato inválido en la línea " + );
		}
	}
	
}
