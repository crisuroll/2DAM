/**
 * Clase A3.
 * @author crisuroll
 */
public class A3 {

	private int numero1;
	private int numero2;
	
	public A3 (int numero1, int numero2) {
		this.numero1 = numero1;
		this.numero2 = numero2;
	}
	
	public boolean equals (A3 a) {
		return (
			this.numero1 = a.numero1 &&
			this.numero2 = a.numero2
				);
	}
	
}
